<% if post.liked_by?(current_user) %>
  <%= link_to(post_des_path(post),remote: true, id: :"dislike-button") do %>
      <i class="fa-lg fas fa-heart icon-btn liked">dislike</i>
  <% end %>
<% else %>
  <%= link_to(post_cre_path(post),remote: true, id: :"like-button") do %>
      <i class="fa-lg fas fa-heart icon-btn not-like">like</i>
  <% end %>
<% end %>