          <h3 onclick="location.href='../head_icon/app_detailed_description'">このアプリの詳細説明</h3>
        </div>
    </aside>
    <main>
      <button onclick="location.href='../home_page/post'"  type="button" id="login_button">+</button>
    </main>
    <footer>
      <img src="../assets/undraw_at_home_octe.svg" onclick="location.href='../home_page/home'">
      <img src="../assets/undraw_questions_re_1fy7.svg" onclick="location.href='../home_page/quiz'" >
      <img src="../assets/undraw_people_search_re_5rre.svg" onclick="location.href='../home_page/search'">
      <img src="../assets/undraw_notify_re_65on.svg" onclick="location.href='../home_page/news'">
      <img src="../assets/undraw_opened_re_i38e.svg" onclick="location.href='../home_page/direct_message'">
    </footer>
  </div>
 </body>
</html>
    
    
 
      


    
   
    

            

        
          
           
